<Window
    x:Class="AvaRxUI.Views.MainWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="clr-namespace:AvaRxUI.Views"
    Title="BrowserBack repro"
    x:DataType="views:MainWindowViewModel">
    <Window.KeyBindings>
        <KeyBinding Gesture="BrowserBack" Command="{Binding LogKeyCommand}" CommandParameter="Command: BrowserBack"></KeyBinding>
        <KeyBinding Gesture="Alt+Left" Command="{Binding LogKeyCommand}" CommandParameter="Command: Alt+Left"></KeyBinding>
    </Window.KeyBindings>
    <Grid>
        <TextBox Name="LogTextBox" Text="{Binding Log}" IsReadOnly="True"></TextBox>
    </Grid>
</Window>