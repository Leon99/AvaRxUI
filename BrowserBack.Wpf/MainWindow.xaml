<Window x:Class="BrowserBack.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrowserBack.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.CommandBindings>
        <CommandBinding Command="BrowseBack" Executed="CommandBinding_OnExecuted"></CommandBinding>
    </Window.CommandBindings>
    <Window.InputBindings>
        <KeyBinding Gesture="Alt+Left"
                    Command="{Binding LogKeyCommand}"
                    CommandParameter="Alt+Left" />
        <KeyBinding Key="BrowserBack"
                    Command="{Binding LogKeyCommand}"
                    CommandParameter="BrowserBack" />
    </Window.InputBindings>
    <Grid>
        <TextBox Name="LogTextBox" Text="{Binding Log}" IsReadOnly="True"></TextBox>
    </Grid>
</Window>